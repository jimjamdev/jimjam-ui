{
    "name": "jimjam-ui",
    "version": "0.0.1",
    "private": false,
    "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint ./src",
      "lint:fix": "next lint --fix",
      "test": "npm run update-version & npm run lint",
      "prepare": "husky install",
      "ui:dev": "npx ladle serve",
      "ui:build": "npx ladle build -o ./docs/ui",
      "ui:preview": "npx ladle preview -o ./docs/ui",
      "cypress": "cypress open",
      "update-version": "npm version patch -git-tag-version false && git add package.json && git add package-lock.json"
    },
    "dependencies": {
      "@stitches/react": "^1.2.8",
      "next": "^13.0.4",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "swiper": "^8.4.4",
      "zod": "^3.19.1"
    },
    "devDependencies": {
      "@ladle/react": "^2.4.5",
      "@types/node": "^18.11.9",
      "@types/react": "^18.0.25",
      "@types/react-dom": "^18.0.9",
      "@typescript-eslint/eslint-plugin": "^5.43.0",
      "@typescript-eslint/parser": "^5.43.0",
      "cypress": "^11.1.0",
      "eslint": "^8.28.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-next": "^13.0.4",
      "eslint-config-prettier": "^8.5.0",
      "eslint-import-resolver-alias": "^1.1.2",
      "eslint-plugin-cypress": "^2.12.1",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jsx-a11y": "^6.6.1",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react-hooks": "^4.6.0",
      "husky": "^8.0.2",
      "lint-staged": "^13.0.3",
      "prettier": "^2.7.1",
      "typescript": "^4.9.3"
    }
  }
  